page {
  background-color: #e9edf0;
  position: static;
}
page .app {
  position: relative;
}
page .app .head-content {
  height: 50%;
  width: 100%;
  position: relative;
}
page .app .head-content open-data {
  display: block;
}
page .app .head-content .top-img {
  filter: blur(10rpx);
  width: 100%;
  height: 375px;
}
page .app .head-content .min-img {
  width: 25%;
  height: 94px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 50%;
  overflow: hidden;
  border: 4rpx solid #fff;
  box-shadow: 8px -4px 10px 0px #aaa;
}
page .app .head-content .top-name {
  position: absolute;
  font-size: 42rpx;
  top: 68%;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #ffffff;
  font-weight: 550;
}
page .app .middle-cntent {
  width: 95%;
  position: fixed;
  top: 50%;
  background: #eeeeee;
  text-align: center;
  left: 50%;
  transform: translate(-50%, 0);
  height: 48vh;
  overflow: hidden;
}
page .app .middle-cntent .bottom > view > view:first-child {
  text-align: left;
  padding: 20rpx;
  font-size: 34rpx;
}
page .app .middle-cntent .bottom .state {
  background: #ffffff;
}
page .app .middle-cntent .bottom .state .state-goods {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
  padding: 15rpx;
}
page .app .middle-cntent .bottom .state .state-goods .all-indent,
page .app .middle-cntent .bottom .state .state-goods .await-goods,
page .app .middle-cntent .bottom .state .state-goods .shipped,
page .app .middle-cntent .bottom .state .state-goods .after-sales {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  width: 23%;
}
page .app .middle-cntent .bottom .state .state-goods .all-indent view,
page .app .middle-cntent .bottom .state .state-goods .await-goods view,
page .app .middle-cntent .bottom .state .state-goods .shipped view,
page .app .middle-cntent .bottom .state .state-goods .after-sales view {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
page .app .middle-cntent .bottom .contact-service {
  text-align: left;
  padding: 20rpx;
}
page .app .middle-cntent .bottom .IPAM,
page .app .middle-cntent .bottom .contact-service,
page .app .middle-cntent .bottom .contact,
page .app .middle-cntent .bottom .nominate {
  width: 100%;
  margin-top: 10rpx;
  background: #fff;
}
page button {
  margin: 30px auto 0;
}
page .result {
  width: 200px;
  margin: 20px auto;
  text-align: center;
}
page .result .headimg {
  width: 200px;
  height: 200px;
  border-radius: 100px;
  margin-bottom: 20px;
}
page .result .headimg image {
  width: 200px;
  height: 200px;
  border-radius: 100px;
}
