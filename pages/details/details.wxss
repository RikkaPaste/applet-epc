@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'), url('data:font/woff;charset=utf-8;base64,') format('woff'), url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 48rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-align: center;
}
.icon-dianhua:before {
  content: "\e8ad";
}
.icon-fenxiang:before {
  content: "\e8b0";
}
.icon-icon-test:before {
  content: "\e63c";
}
.icon-icon-test1:before {
  content: "\e650";
}
.icon-icon-test2:before {
  content: "\e657";
}
.icon-icon-test3:before {
  content: "\e658";
}
page {
  position: static;
  background-color: #eef2f5;
  font-family: Arial, Helvetica, sans-serif;
}
page image {
  width: 100%;
  height: 100%;
}
page .goodsPrice {
  margin-top: 15rpx;
  padding: 25rpx;
  font-size: 34rpx;
  color: rgba(163, 26, 2, 0.8);
}
page .goodsName {
  padding: 30rpx;
  border: 3rpx solid rgba(0, 0, 0, 0.1);
  font-size: 34rpx;
}
page .gdsdetail {
  padding: 16rpx 16rpx 130rpx;
}
page .gdsdetail .label {
  text-align: center;
  padding: 20rpx;
  margin: 20rpx auto;
  background: #ffffff;
  border-radius: 10rpx;
  position: sticky;
  top: 0;
  border-bottom: 3rpx solid rgba(0, 0, 0, 0.4);
}
page .bottom {
  background: white;
  border-top: 3rpx solid rgba(0, 0, 0, 0.4);
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: nowrap;
}
page .bottom .dh,
page .bottom .fx {
  width: 20%;
  height: auto;
  text-align: center;
}
page .bottom .buttonfx,
page .bottom .buttondh {
  font-weight: 0;
  color: #000;
  font-size: 20rpx;
  background: none;
}
page .bottom .bt {
  display: flex;
  width: 40%;
  height: 100%;
  font-size: 30rpx;
  color: white;
  justify-content: center;
  align-items: center;
}
page .bottom .addcat {
  border-radius: 20rpx 0 0 20rpx;
  background: rgba(247, 42, 6, 0.5);
}
page .bottom .buy {
  border-radius: 0 20rpx 20rpx 0;
  background: #df0404;
}
